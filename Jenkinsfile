// Jenkinsfile
pipeline {
    agent any
  
  stages {
    
       stage('git clone') {
             steps {
                sh 'sudo rm -r *;sudo git clone https://github.com/aleti-pavan/jenkins.git'
                  }
                           }
    
    stage('Compile')  {
         steps{
           echo "Compilation started"
             }
                      }

         }
    }
