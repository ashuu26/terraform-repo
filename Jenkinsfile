pipeline {
     agent any
 
 stages {
    
	 stage ('Git Checkout'){
		 steps {
	         sh 'sudo rm -rf /home/ubuntu/terraform-repo'
                 sh 'sudo git clone https://github.com/vivekdevopshub/terraform-repo.git /home/ubuntu'
                      }
	                       }
	 
	

     }
 
}
